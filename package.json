{
  "name": "root",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "type-check": "turbo run type-check --filter=ts-ics",
    "lint": "turbo run lint --filter=ts-ics",
    "dev": "turbo run dev --filter=ts-ics",
    "build": "turbo run build --filter=ts-ics",
    "test": "turbo run test --filter=ts-ics",
    "dev/docs": "turbo run start --filter=docs",
    "build/docs": "turbo run build --filter=docs"
  },
  "author": "Neuvernetzung Medienagentur UG",
  "packageManager": "npm@10.2.1",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@neuvernetzung/eslint-config-custom": "^1.1.6",
    "@neuvernetzung/tsconfig": "^1.0.2",
    "@types/node": "^20.10.4",
    "turbo": "^1.11.2"
  }
}
